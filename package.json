{
  "name": "dotfiles",
  "version": "1.0.0",
  "description": "Personal dotfiles for macOS and Linux terminal productivity",
  "type": "module",
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "setup": "node --experimental-strip-types setup.ts",
    "setup:interactive": "node --experimental-strip-types setup.ts --interactive",
    "setup:force": "node --experimental-strip-types setup.ts --force-overwrite",
    "setup:append": "node --experimental-strip-types setup.ts --append",
    "setup:no-packages": "node --experimental-strip-types setup.ts --skip-packages",
    "test": "node --experimental-strip-types --test",
    "test:watch": "node --experimental-strip-types --test --watch",
    "typecheck": "tsc --noEmit",
    "ci:setup": "node --experimental-strip-types setup.ts --non-interactive --force-overwrite"
  },
  "keywords": [
    "dotfiles",
    "macos",
    "linux",
    "terminal",
    "productivity",
    "tmux",
    "zsh"
  ],
  "author": "yordan.kanchelov",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1",
    "which": "^4.0.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^22.14.0",
    "@types/which": "^3.0.4",
    "prettier": "^3.1.1",
    "typescript": "^5.8.2"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
